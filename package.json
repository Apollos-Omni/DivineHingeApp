{
    "name": "divinehingeapp",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "audit:prod": "npm audit --production",
        "audit:all": "npm audit",
        "test": "vitest",
        "test:ci": "vitest run --coverage",
        "lint": "eslint . --ext .ts,.tsx",
        "format": "prettier --write .",
        "typecheck": "tsc --noEmit",
        "circular": "madge --circular src",
        "prune": "ts-prune --error",
        "doctor": "expo-doctor --non-interactive",
        "deps:check": "depcheck",
        "postinstall": "patch-package",
        "start:clear": "expo start --clear",
        "format:check": "prettier --check .",
        "validate:env": "node scripts/dx/validate-env.mjs",
        "validate:appconfig": "node scripts/dx/check-config.mjs",
        "android:prebuild:clean": "expo prebuild --clean --platform android",
        "android:run": "expo run:android",
        "android:emulator": "powershell -ExecutionPolicy Bypass -File scripts/dx/android-fix.ps1",
        "android:adb:reset": "powershell -ExecutionPolicy Bypass -File scripts/dx/adb-reset.ps1",
        "clean": "node scripts/dx/clean.mjs",
        "build:android:preview": "eas build --platform android --profile preview --non-interactive",
        "build:android:prod": "eas build --platform android --profile production --non-interactive",
        "update:preview": "eas update --channel preview --non-interactive",
        "update:prod": "eas update --channel production --non-interactive",
        "dx": "node scripts/dx/triage.mjs",
        "prepare": "husky",
        "test:run": "vitest run",
        "fmt": "prettier --write ."
    },
    "dependencies": {
        "@auth0/auth0-spa-js": "^2.4.1",
        "@logrocket/react-native": "^1.57.0",
